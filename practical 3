#include<stdio.h>

void main(){
printf("Hello There");
}



#include<stdio.h>

void main(){
int a,b,sum;
printf("A = ");
scanf("%d",&a);
printf("B = ");
scanf("%d",&b);
sum = a+b;
printf("Sum = %d\n",sum);
}



#include<stdio.h>

void main(){
int a,b,diff;
printf("A = ");
scanf("%d",&a);
printf("B = ");
scanf("%d",&b);
diff=a-b;
printf("Difference = %d\n",diff);
}

#include<stdio.h>

void main(){
int a,b,prod;
printf("A = ");
scanf("%d",&a);
printf("B = ");
scanf("%d",&b);
prod=a*b;
printf("Product = %d\n",prod);
}


#include <stdio.h>

int main() {
    int a, b, div;

    printf("A = ");
    scanf("%d", &a);

    printf("B = ");
    scanf("%d", &b);

    if (b == 0) {
        printf("Division by zero is not allowed\n");
    } else {
        div = a / b;
        printf("Div = %d\n", div);
    }

    return 0;
}
